{
    "version": 2,
    "routes": [
        {
            "src": "/v1/user/.*",
            "dest": "/api/user.js"
        },
        {
            "src": "/v1/note/.*",
            "dest": "/api/note.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "functions": {
        "api/user": {
            "handler": "user.js"
        },
        "api/note": {
            "handler": "note.js"
        }
    }
}